# Configure kernel command line parameters.
#Kernel-Cmdline: quiet splash

# Set a preferred compression level for the kernel.
#Kernel-Compression: none lz4 lzma

# Whether to use initramfs or not. Adds 'noinitrd' to command line.
#Ignore-Initramfs: no

# Override automatic machine detection.
#Machine: <machine>

# Use different keys for signing and verification.
#Vboot-Keyblock: /usr/share/vboot/devkeys/kernel.keyblock
#Vboot-Public-Key: /usr/share/vboot/devkeys/kernel_subkey.vbpubk
#Vboot-Private-Key: /usr/share/vboot/devkeys/kernel_data_key.vbprivk
